import cv2 
   
 
path = r"C:\Users\yasee\OneDrive\Pictures\wall.jpeg"
   

image = cv2.imread(path)
   

window_name = 'Image'
  

start_point = (5, 5)
  

end_point = (220, 220)
  

color = (255, 0, 0)
  

thickness = 2

image = cv2.rectangle(image, start_point, end_point, color, thickness)
  
 
cv2.imshow(window_name, image) 
